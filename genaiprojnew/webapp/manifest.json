{
  "_version": "1.76.0",
  "sap.app": {
    "id": "genaiprojnew",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.6",
      "toolsId": "bf20ff67-2170-4a2e-a9c1-44bd2000319d"
    },
    "crossNavigation": {
      "inbounds": {
        "Zgenaisemobjnew-display": {
          "semanticObject": "Zgenaisemobjnew",
          "action": "display",
          "title": "{{Zgenaisemobjnew-display.flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    },
    "dataSources": {
  "cockpit": {
    "uri": "/cockpit/",
    "type": "OData",
    "settings": {
      "annotations": [],
      "localUri": "localService/cockpit/metadata.xml",
      "odataVersion": "4.0"
    }
  }
}
 
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "genaiprojnew.i18n.i18n"
        }
      },
     
"cockpit": {
    "dataSource": "cockpit",
    "settings": {
      "synchronizationMode": "None",
      "operationMode": "Server",
      "autoExpandSelect": true
    }
  }
 
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "controlId": "flexibleColumnLayout",
        "controlAggregation": "midColumnPages",
        "viewType": "XML",
        "async": true,
        "transition": "slide"
      },
      "routes": [
        {
          "pattern": "",
          "name": "master",
          "target": [
            "master","detail"
          ]
        },
        {
          "pattern": "detail/{tab}",
          "name": "detail",
          "target": [
            "master","detail"
          ]
        },
        {
          "pattern": "detail/{tab}/detailDetail",
          "name": "detailDetail",
          "target": [
            "master","detailDetail"
          ]
        }
      ],
      "targets": {
        "master": {
          "viewName": "genaiprojnew.view.Master",
          "viewId": "master",
          "viewLevel": 0,
          "controlAggregation": "beginColumnPages"
        },
        "detail": {
          "viewName": "genaiprojnew.view.Detail",
          "viewId": "detail",
          "viewLevel": 1,
          "controlAggregation": "midColumnPages"
        },
        "detailDetail": {
          "viewName": "genaiprojnew.view.DetailDetail",
          "viewId": "detailDetail",
          "viewLevel": 2,
          "controlAggregation": "midColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "genaiprojnew.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  },
  "sap.cloud":{
    "public": true,
    "service": "Zgenaisemobjnew"
  }
}
 
 