<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="idloginListDialog" title="User List Log" resizable="true">
       
<content>
        <Table id="loginList"
		inset="false"
		items="{loginModel>/}">
	
		<columns>
			
                    <Column width="10em">
                        <Text text="User Name" />
                    </Column>
                    <Column width="15em">
                        <Text text="Email" />
                    </Column>
                    <Column width="15em">
                        <Text text="Project" />
                    </Column>
                    <Column width="10em">
                        <Text text="Date" />
                    </Column>

                    <Column width="10em">
                        <Text text="Total duration" />
                    </Column>
                    <Column width="10em">
                        <Text text="No. Of Session" />
                    </Column>
			
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{loginModel>USERNAME}" />
                            <Text text="{loginModel>EMAIL_ID}" />

                            <Text text="{loginModel>project}" />

                             <Text text="{loginModel>date}"/>                    
                            
                            <Text text="{loginModel>totalDuration}" />
                            
                            <Text text="{loginModel>totalSessions}" />

					
				
				</cells>
			</ColumnListItem>
		</items>
	</Table>
     </content>
      <buttons>
            <Button text="Close" press="onOpenlistCancel"/>
        </buttons> 
       
    </Dialog>
</core:FragmentDefinition>